{
  "name": "@habityzer/nuxt-kinde-auth",
  "version": "1.3.3",
  "description": "Kinde authentication module for Nuxt 4",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build && pnpm client:build",
    "client:build": "nuxi generate client",
    "dev": "pnpm play:dev",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare client",
    "prepare": "nuxi prepare client",
    "play:dev": "nuxi dev playground",
    "play:prod": "pnpm prepack && nuxi dev playground",
    "release": "pnpm lint && pnpm test && pnpm prepack && semantic-release",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@kinde-oss/kinde-typescript-sdk": "^2.13.0",
    "@nuxt/devtools-kit": "^2.6.5",
    "@nuxt/kit": "^4.1.3",
    "defu": "^6.1.4",
    "sirv": "^3.0.2"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.2.14",
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/devtools-ui-kit": "^2.6.5",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/eslint-plugin": "^1.15.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "changelogen": "^0.6.2",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.37.0",
    "nuxt": "^4.1.3",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
