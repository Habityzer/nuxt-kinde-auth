{
  "name": "@habityzer/nuxt-kinde-auth",
  "version": "1.1.0",
  "description": "Kinde authentication module for Nuxt 4",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub",
    "build": "nuxt-module-build build",
    "typecheck": "nuxi typecheck",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepublishOnly": "pnpm build && pnpm test",
    "release": "pnpm build && pnpm test && HUSKY=0 semantic-release"
  },
  "dependencies": {
    "@kinde-oss/kinde-typescript-sdk": "^2.13.0",
    "@nuxt/kit": "^4.1.3",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.5.7",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^4.1.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^22.10.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.37.0",
    "h3": "^1.13.0",
    "nuxt": "^4.1.3",
    "semantic-release": "^24.2.9",
    "typescript": "^5.9.3",
    "unbuild": "^2.0.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "peerDependencies": {
    "nuxt": "^4.0.0"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "kinde",
    "authentication",
    "oauth",
    "auth"
  ],
  "author": "Vazgen Manukyan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Habityzer/nuxt-kinde-auth.git"
  },
  "homepage": "https://github.com/Habityzer/nuxt-kinde-auth#readme",
  "bugs": {
    "url": "https://github.com/Habityzer/nuxt-kinde-auth/issues"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
